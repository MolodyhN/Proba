#include <iostream>
#include <string>
#include <cmath>
using namespace std;

#include <stdio.h>
#include <stdarg.h>

int xer(int count, ...)
{
    int s = 0;
    va_list arg;            // указатель на параметр
    va_start(arg, count);   // получение адреса первого вариадического параметра 

    for (int i = 0; i < count; ++i) {
        int b= va_arg(arg, int); // получение значение вариадического параметра и переход к следующему параметру
		int s = 0;
		int k = 0;
		while (b > 1) {
			s += b % 10 * pow(8, k);
			k++;
			b = b / 10;
		}
		cout << s << " ";
    }

    va_end(arg);               // завершение процедуры перебора вариадических параметров

    return 0;
}

int main() {
	xer(5, 66, 4, 34, 65, 765);
}
