#include <iostream>
using namespace std;
const int n=10;
int a[n];
int i;
int j;
int temp;

int main(){
	cout << "I:";
	cin >> i;
	cout << endl;
	cout << "j:";
	cin >> j;
	for (int i = 0; i < n; i++) {
		a[i] = rand() % 100 - 50;
		cout << a[i] << " ";
	}
	cout << endl;
	while (i < j) {
		temp = a[i];
		a[i] = a[j];
		a[j] = temp;
		i++;
		j--;
	}
	for (int i = 0; i < n; i++) {
		cout << a[i] << " ";
	}
	return 0;

}

