#include <iostream>
#include <ctime>
using namespace std;
const int n = 10;

int main() {
	setlocale(LC_ALL, "Ru");
	int max;
	int i;
	int s = 1;
	int j;
	int a[n];
	for (int i = 0; i < n; i++) {
		a[i] = rand() % 200 - 100;
	}
	max = a[0];
	for (int i = 1; i < n; i++) {
		if (a[i] > max) {
			max = a[i];
			s = 1;
			j = i;
		}
		else {
			if (a[i] == max) {
				s++;
			}
		}
	}
	for (int i = 0; i < n; i++) {
		cout << a[i] << endl;
	}
	cout << "Макс:" << max << endl;
	cout << "Номер макс:" << j + 1 << endl;
	cout << "Кол-во макс:" << s;


}
