#include <iostream>
using namespace std;
const int n = 10;
int temp;
int k;
int a[n];
int main()
{
	for (int i = 0; i < n; i++) {
		a[i] = rand() % 100 - 50;
		cout << a[i] << " ";
	}
	cout << "Bbog:";
	cin >> k;
	for (int i = 0; i < k; i++) {
		temp = a[0];
		for (int j = 0; j < n - 1; j++) {
			a[j] = a[j + 1];
		}
		a[n - 1] = temp;
	}
	for (int i = 0; i < n; i++) {
		cout << a[i] << " ";
	}
	return 0;
}
