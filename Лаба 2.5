#include <iostream>
using namespace std;
const int n = 10;
const int m = 10;
int a[n][m];
int b[n];
int s = 0;
int mx = 0;
int main() {
    setlocale(LC_ALL, "Ru");
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < m - 1; j++) {
            a[i][j] = rand() % 100 - 50;
            cout << a[i][j] << " ";
        }
        cout << endl;
    }
    int z = n;
    for (int i = 0; i < m - 2; i++) {
        s = 0;
        z -= 1;
        for (int j = 0; j < z; j++) {
            s += a[i][j];
        }
        b[i] = s;
    }
    for (int i = 0; i < m; i++) {
        if (b[i] > mx) {
            mx = b[i];
        }
    }
    cout << "Максимальная сумма:" << mx;
}
